{
  "rewrites": [
    {
      "source": "/api/auth/:path*",
      "destination": "/api/auth/:path*"
    },
    {
      "source": "/login",
      "destination": "/login"
    },
    {
      "source": "/account",
      "destination": "/account"
    },
    {
      "source": "/:path((?!api|login|account).*)",
      "destination": "https://alikernel.blogspot.com/:path*"
    }
  ]
}
